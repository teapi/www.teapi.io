noselect()
  -webkit-user-select none
  -moz-user-select none
  -ms-user-select none

.viewer
  position relative
  padding-bottom 430px
  margin-bottom 50px
  &.loading
    opacity 0.4
  .url
    width 100%
    border 1px solid #eee
    // border-bottom none
    padding 7px 10px
    box-sizing border-box
    font-size 14px
    background #fff
  ul
    list-style none
  .searches
    noselect()
    bottom 0
    position absolute
    z-index 0
    background #666
    color #fff
    font-size 0.9em
    min-width 200px
    &.many
      cursor pointer
    &.many:before
      position absolute
      bottom 8px
      right 12px
      font-size 12px
      content: "\25BC";
    &.show
      z-index 3
    &.show:before
      content: "\25B2";
  .searches li
    padding 0 50px 0 10px
    height 30px
    line-height 30px
  .searches.many li:last-of-type
    border-top 1px solid #000
  .searches.many li:last-of-type
    border-top 1px solid #000
  .searches.show li:not(:last-of-type):hover
    background #999

.viewer .result
  background #f6f6f6
  box-sizing border-box
  border 1px solid #f0f0f0
  border-bottom 0
  width 100%
  position absolute
  height 400px
  z-index 2
  overflow auto
  font-size 0.8em
  line-height 20px
  white-space  pre-wrap
  & .key
    color #444
  & .string
    color #5c833b
  & .null
    color #a55840
  & .number
    color #33aca8
